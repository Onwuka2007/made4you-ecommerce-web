{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/", "destination": "/src/pages/index.html" },
    { "source": "/home", "destination": "/src/pages/index.html" },
    { "source": "/:path", "destination": "/src/pages/:path.html" }
  ],
  "redirects": [
    { "source": "/index", "destination": "/home", "permanent": true }
  ]
}
